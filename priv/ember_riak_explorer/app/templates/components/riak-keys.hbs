<table class="table">
<tbody>
{{#each obj in keys}}
    <tr>
        <td>
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    {{#link-to 'riak-object'
                        (query-params cluster_id=obj.bucket.clusterId
                            bucket_type_id=obj.bucket.bucketTypeId
                            bucket_id=obj.bucket.name
                            object_key=obj.key)}}
                        {{obj.key}}{{/link-to}}
                </div>
            </div>
            <div class="row">
                <div class="col-md-12">
                </div>
            </div>
        </div>
        </td>
    </tr>
{{else}}
    No keys found.
{{/each}}
</tbody>
</table>
