<table class="table">
<tbody>
{{#each keys as |obj|}}
    <tr>
        <td>
        <div class="container">
            <div class="row">
                <div>
                    {{#if obj.markedDeleted}}
                        <strike>{{obj.key}}</strike>
                    {{else}}
                        {{#if obj.bucket.props.isCounter}}
                            {{#link-to 'riak-object.counter' obj}}
                                {{obj.key}}{{/link-to}}
                        {{else}}
                            {{#link-to 'riak-object' obj}}
                                {{obj.key}}{{/link-to}}
                        {{/if}}
                    {{/if}}
                </div>
            </div>
        </div>
        </td>
    </tr>
{{else}}
    No keys found.
{{/each}}
</tbody>
</table>
