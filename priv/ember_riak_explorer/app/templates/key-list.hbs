<h4>Key List</h4>

<div class="container">
    <div class="row">
        <div class="col-md-7">
            <strong>Bucket Type:</strong>
            {{model.bucket_type_id}}
        </div>
            <div class="col-md-7">
                <strong>Bucket:</strong>
                {{model.bucket_id}}
            </div>
        <div class="col-md-5">
            <strong>Cluster:</strong>
            {{model.cluster_id}}
        </div>
    </div>
</div>

<div class="container" style="margin-top: 2em;">
    Displaying
    <strong>{{model.count}}</strong>
    keys out of
    <strong>{{model.total}}</strong>
    total.
</div>

<div class="container" style="padding-top: 2em;">
    {{#riak-keys deleteObject="deleteObject" keys=model.key_list}}{{/riak-keys}}
</div>
