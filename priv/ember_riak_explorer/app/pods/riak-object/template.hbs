{{#object-location object=model isEditing=false}}{{/object-location}}

<br />
{{#if model.isLoaded}}
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                {{#object-headers headers=model.headersIndexes
                    title='Secondary Indexes'}}{{/object-headers}}
            </div>
        </div>
        <div class="row">
            <div class="col-md-12">
                {{#object-headers headers=model.headersCustom
                    title='Custom Headers'}}{{/object-headers}}
            </div>
        </div>
    </div>

    <br />
    <div class="container">
        <h4>Contents</h4>
        <div class="row">
            <div class="col-md-2">
                <a href="{{model.rawUrl}}" class="btn btn-xs btn-primary">
                    <span class="glyphicon glyphicon-download" aria-hidden="true"></span>
                    View raw</a>
            </div>
            <div class="col-md-10">
                Content Type: <b>{{model.contentType}}</b>
            </div>
        </div>
    </div>
    <br />
    {{#if model.contents}}
    <pre><code>{{model.contents}}</code></pre>
    {{/if}}

    {{#object-version object=model}}{{/object-version}}
{{else}}
    {{loading-spinner}}
{{/if}}
